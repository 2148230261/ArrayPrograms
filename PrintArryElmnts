public class PrintArryElmnts {
	 public static void main(String[] args) {  
	         
	        int [] a = new int [] {1, 2, 3, 4, 5};  
	        System.out.println("Elements of given array: ");  
	         
	        for (int i = 0; i < a.length; i++) {  
	            System.out.print(a[i] + " ");  
	        }  
	    }  
	}  
