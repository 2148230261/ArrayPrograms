public class Copy1ArrayElmnt {
	 public static void main(String[] args) {  
         
	        int [] a = new int [] { 2, 3, 4, 5};  
	        System.out.println(" FIRST ARRAY Element of given array: ");  
	         
	        for (int i = 0; i <1; i++) {  
	            System.out.print(a[i] + " ");  
	        }  
	    }  
	}  



